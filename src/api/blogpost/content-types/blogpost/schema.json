{
  "kind": "collectionType",
  "collectionName": "blogposts",
  "info": {
    "singularName": "blogpost",
    "pluralName": "blogposts",
    "displayName": "blogpost",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext"
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "blogposts"
    },
    "authors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "admin::user"
    },
    "seo_description": {
      "type": "string"
    },
    "seo_title": {
      "type": "string"
    },
    "date": {
      "type": "date"
    },
    "display_locations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::display-location.display-location",
      "inversedBy": "blogposts"
    }
  }
}
